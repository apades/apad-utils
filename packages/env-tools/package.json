{
  "name": "@apad/env-tools",
  "version": "2.0.2",
  "description": "Reading .env file and easy to use it",
  "author": "apades",
  "license": "MIT",
  "homepage": "https://github.com/apades/apad-utils/tree/master/packages/env-tools",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apades/apad-utils.git",
    "directory": "packages/env-tools"
  },
  "bugs": {
    "url": "https://github.com/apades/apad-utils/issues"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "build": "rm lib -drf && tsup index.ts web.ts env.ts bundler.ts --clean --out-dir lib --dts --format esm,cjs",
    "build:dependencies": "cd ../.. && turbo run build --filter=@apad/env-tools^..."
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./web": {
      "import": "./lib/web.js",
      "require": "./lib/web.cjs"
    },
    "./bundler": {
      "import": "./lib/bundler.js",
      "require": "./lib/bundler.cjs"
    },
    "./env": {
      "import": "./lib/env.js",
      "require": "./lib/env.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/index.d.ts"
      ],
      "web": [
        "lib/web.d.ts"
      ],
      "bundler": [
        "lib/bundler.d.ts"
      ],
      "env": [
        "lib/env.d.ts"
      ]
    }
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "fs-extra": "^11.1.0"
  },
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "dotenv",
    "env",
    ".env",
    "environment",
    "config"
  ]
}