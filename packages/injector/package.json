{
  "name": "@apad/injector",
  "version": "0.0.1",
  "description": "注入/替换 系统API，方便调试所有底层API，且支持在web插件中使用甚至可以支持bg",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apades/apad-utils.git",
    "directory": "packages/injector"
  },
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/apades/apad-utils/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "dev": "vite serve -c vite.config.js test",
    "build:client": "tsup ./src/core/client.ts --out-dir lib --dts --format esm,cjs,iife --global-name injectorClient --minify --shims --treeshake",
    "build:injector": "tsup ./src/core/injector.ts --out-dir lib --dts --format esm,cjs,iife --global-name injectorService --minify --shims --treeshake",
    "build:messageRelay": "tsup ./src/core/messageRelay.ts --clean --out-dir lib --dts --format esm,cjs --global-name messageRelay",
    "build-script": "tsup $1 --out-dir lib --dts --format esm,cjs --global-name $2"
  },
  "keywords": [
    "injection",
    "js-inject"
  ],
  "author": "apades",
  "license": "MIT",
  "dependencies": {
    "webext-bridge": "^6.0.1"
  }
}
