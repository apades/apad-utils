{
  "name": "@apad/injector",
  "version": "0.0.1",
  "description": "注入/替换 系统API，方便调试所有底层API，且支持在web插件中使用甚至可以支持bg",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apades/apad-utils.git",
    "directory": "packages/injector"
  },
  "exports": {
    "./*": {
      "import": "./lib/*.mjs",
      "require": "./lib/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/*.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/apades/apad-utils/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "dev": "vite serve -c vite.config.js test",
    "build": "run-p build:*",
    "build:client": "pnpm run build-script client.ts injectorClient",
    "build:injector": "pnpm run build-script injector.ts injectorService",
    "build:ext-bg-messager-bg": "pnpm run build-script-ext-bg background.ts extBg_background",
    "build:ext-bg-messager-cs": "pnpm run build-script-ext-bg content-script.ts extBg_cs",
    "build:ext-bg-messager-wd": "pnpm run build-script-ext-bg window.ts extBg_window",
    "build----": "----------",
    "build-script": "sh -c \"tsup ./src/core/${0} --out-dir lib --dts --format esm,cjs,iife --global-name $1 --treeshake --shims --minify --env.NODE_ENV production\"",
    "build-script-ext-bg": "sh -c \"tsup ./src/core/ext-bg-messager/${0} --out-dir lib/ext-bg-messager --dts --format esm,cjs,iife --global-name $1 --treeshake --shims --minify --env.NODE_ENV production --target esnext\""
  },
  "keywords": [
    "injection",
    "js-inject"
  ],
  "author": "apades",
  "license": "MIT",
  "dependencies": {
    "webext-bridge": "^6.0.1",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@types/webextension-polyfill": "^0.10.4"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  }
}
