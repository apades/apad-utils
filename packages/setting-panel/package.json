{
  "name": "@apad/setting-panel",
  "version": "0.0.1",
  "description": "开发配套设置工具",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apades/apad-utils.git",
    "directory": "packages/setting-panel"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/apades/apad-utils/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "dev": "vite serve -c vite.config.js test",
    "build:testWeb": "vite build -c vite.config.js test",
    "build": "pnpm run clearLib && pnpm run build:iife && pnpm run build:m",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clearLib": "rm -rf ./lib && rm -rf ./dist",
    "build:iife": "tsup src/index.tsx --dts --format iife --global-name settingPanel --config ./build/tsup.iife.ts --minify --metafile --env.NODE_ENV production lib",
    "build:m": "tsup src/index.tsx --dts --format esm,cjs --config ./build/tsup.m.ts --minify --metafile --env.NODE_ENV production lib"
  },
  "keywords": [],
  "author": "apades",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.2.5",
    "autoprefixer": "^10.4.14",
    "esbuild-plugin-replace": "^1.4.0",
    "mobx": "^6.10.0",
    "mobx-react": "^9.0.0",
    "postcss": "^8.4.27",
    "preact": "^10.16.0",
    "preact-compat": "^3.19.0",
    "tailwindcss": "^3.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
